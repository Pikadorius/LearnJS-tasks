function camelize(str) {
  return str
            .split('-') //разбивает на отдельные слова (делатель "_")
            .map((x, index) => index===0?x:x[0].toUpperCase()+x.slice(1)) //делает новый массив со словами с большой буквы. Если индекс=0 - не меняет слово
            .join('');  //собирает строку из массива
}
